generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ShortenURL {
  id          String   @id @default(uuid())
  shortCode   String   @unique
  shortenURL  String
  originalURL String
  ownerEmail  String
  secret      String
  clicks      Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
